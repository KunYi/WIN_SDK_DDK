NT_UP=1
BROWSER_INFO=1
RCNOFONTMAP=1

LINK_LIB_IGNORE=4210
MSC_OPTIMIZATION = /Oxs /Oi

INCLUDES=$(DRIVERNETWORKS)\..\common\include\stl;\
	$(DRIVERNETWORKS)\include;\
	$(BASEDIR)\inc;\
	$(BASEDIR)\inc\ddk;\
	$(BASEDIR)\src\network\inc;\
	$(DRIVERWORKS)\include

SOURCES=coisdnadap.cpp bchannel.cpp card.cpp dchannel.cpp port.cpp coisdndrv.cpp coisdnoids.cpp coisdn.rc

