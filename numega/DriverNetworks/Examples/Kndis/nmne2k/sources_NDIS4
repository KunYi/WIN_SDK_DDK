# SOURCES for use with BUILD command

TARGETNAME=NMNE2k
TARGETPATH=obj
TARGETTYPE=DRIVER

!IF ("$(DDKBUILDENV)"=="chk") || ("$(DDKBUILDENV)"=="checked")
BUILDTYPE = CHECKED
!ELSE
BUILDTYPE = FREE
!ENDIF

TARGETLIBS= $(DRIVERNETWORKS)\lib\*\$(BUILDTYPE)\kndis4mp.lib $(DDK_LIB_PATH)\ndis.lib

C_DEFINES= -DNDIS_MINIPORT_DRIVER -DNDIS40_MINIPORT -DBINARY_COMPATIBLE=1 
BROWSER_INFO=1
RCNOFONTMAP=1

LINK_LIB_IGNORE=4210
MSC_OPTIMIZATION = /Oxs /Oi

INCLUDES=$(DRIVERNETWORKS)\include;\
	$(BASEDIR)\inc;\
	$(BASEDIR)\inc\ddk;\
	$(BASEDIR)\src\network\inc;\
	$(DRIVERWORKS)\include

SOURCES=nmne2kadap.cpp nmne2kdrv.cpp nmne2koids.cpp nmne2k.rc

