		"A smokey glass bottle.",
		emptyStr,
		bottleRetail_c,
		bottleWholesale_c,
		n_grassRoomHut18
	);
	mov( &verb_use, possessionEAX.actions[ uns32( v_use )*4 ] );

	// put dagger in cottage r#24:

	createWeapon
	(
		"dagger",
		"Metal Dagger",
		"A small metal dagger with a jeweled hilt.",
		"Do not run while holding this knife.",
		daggerRetail_c,
		daggerWholesale_c,
		daggerPower_c,
		rooms[ r_abandonedCottage24 ]
	);

	// put battleaxe in grassroof hut r#18:

	createWeapon
	(
		"battleaxe",
		"Metal Battleaxe",
		"A sturdy metal battleaxe. It looks sharp and lethal!",
