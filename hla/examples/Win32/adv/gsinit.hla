// gsinit.hla
//
// Code to initialize the room data structures and other
// game-specific objects.

unit gsinit_hla;
?@nodisplay := true;
?@nostackalign := true;
#includeonce( "stdlib.hhf" )
#includeonce( "age.hhf" )
#includeonce( "gamespecific.hhf" )



static
	goblinAgent		:agentPtr;
	sorcererAgent	:agentPtr;
	vampireAgent	:agentPtr;
	butlerAgent		:agentPtr;
	werewolfAgent	:agentPtr;
	hellhoundAgent	:agentPtr;
	dragonAgent		:agentPtr;

	emptyStr		:string := "";
	

// Okay, begin creating the room objects for this game.
// Begin by defining some string constants and macros
// that we can use to automate the initialization of the
// rooms.

const

	dirStrs :=
		[
			"north",
			"south",
			"east",
			"west",
			"northeast",
			"northwest",
			"southeast",
			"southwest",
			"up",
			"down"
		];
		
	nothingToSeeStrs :=
		[
			"NothingToSeeNorth",
			"NothingToSeeSouth",
			"NothingToSeeEast",
			"NothingToSeeWest",
			"NothingToSeeNorthEast",
			"NothingToSeeNorthWest",
			"NothingToSeeSouthEast",
			"NothingToSeeSouthWest",
			"NothingToSeeUp",
			"NothingToSeeDown"
		];
		
	seeAPathStrs :=
		[
			"SeeAPathLeadingNorth",
			"SeeAPathLeadingSouth",
			"SeeAPathLeadingEast",
			"SeeAPathLeadingWest",
			"SeeAPathLeadingNorthEast",
			"SeeAPathLeadingNorthWest",
			"SeeAPathLeadingSouthEast",
			"SeeAPathLeadingSouthWest",
			"SeeAPathLeadingUp",
			"SeeAPathLeadingDown"
		];
		
	noPathStrs :=
		[
			"NoPathLeadingNorth",
			"NoPathLeadingSouth",
			"NoPathLeadingEast",
			"NoPathLeadingWest",
			"NoPathLeadingNorthEast",
			"NoPathLeadingNorthWest",
			"NoPathLeadingSouthEast",
			"NoPathLeadingSouthWest",
			"NoPathLeadingUp",
			"NoPathLeadingDown"
		];
		
	goStrs :=
		[
			"GoingNorth",
			"GoingSouth",
			"GoingEast",
			"GoingWest",
			"GoingNorthEast",
			"GoingNorthWest",
			"GoingSouthEast",
			"GoingSouthWest",
			"GoingUp",
			"GoingDown"
		];
		
// The following macro automatically generates
// all the initialization strings for the rooms.
// You supply only the directions where the player
// can actually travel and the macro generates the rest.

#macro initRoomStrs( _dirs_[] ): _present_, _index_, _last_;

	?_last_ := (@elements( dirStrs ) - 1);
	#for( _index_ := 0 to _last_ )

		?_present_ :boolean := false;
		#for( _thisDir_ in _dirs_ )
		
			?_present_ := _present_ | (_thisDir_ = dirStrs[ _index_ ]);
			
		#endfor
		#if( _present_ )
		
			@text( seeAPathStrs[ _index_ ]),
			@text( goStrs[ _index_ ])
			
		#else
		
			@text( nothingToSeeStrs[ _index_ ]),
			@text( noPathStrs[ _index_ ])
		
		#endif
		#if( _index_ <> _last_ )
			,
		#endif
		
	#endfor
	
#endmacro


//////////////////////////////////////////////////////////////////////////////
//
// initializePlayer-
//
//	Game-specific initialization of the game's player

_procedure( initializePlayer)( p:playerPtr );
begin initializePlayer;

	push( eax );
	push( ebx );
	push( esi );

	mov( p, esi );	// Get the player.

	stdout.put( EnterYourName );
	stdin.a_gets();
	mov( eax, ebx );
	str.a_cat( eax, AnAgePlayer );

	playerESI.initPlayer
	(
		(type string ebx),
		(type string eax),
		n_guild10,				// Start player in guild room.
		0,						// initial score
		playerInitialFood_c,	// initial food level
		playerInitialHealth_c,	// initial health
		playerInitialPower_c,	// initial power (experience)
		playerInitialWealth_c	// initial wealth (200 gold pieces)
	);
	mov( lantern_quest_c, playerESI.quest );
	
	// We need to patch into all the "GO" verbs so we can
	// determine when certain quests are complete.
	
	#macro setgo( n );
	
		mov( n, esi );
		mov(&gs_go, nounESI.actions[ uns32( v_go )*4 ] );
		
	#endmacro
	
	setgo( northNoun );
	setgo( southNoun );		
	setgo( eastNoun );		
	setgo( westNoun );		
	setgo( northEastNoun );
	setgo( northWestNoun );
	setgo( southEastNoun );
	setgo( southWestNoun );
	setgo( upNoun );			
	setgo( downNoun );		

	pop( esi );
	pop( ebx );
	pop( eax );
		 
end initializePlayer;






