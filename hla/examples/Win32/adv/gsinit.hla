		keyRetail_c,
		keyWholesale_c,
		n_storageRoom9
	);
	mov( &verb_use, possessionEAX.actions[ uns32( v_use )*4 ] );
	
	// The key is used to unlock the GUILD room, so attach the
	// key to the door impediment here:
	
	mov( rooms[ r_villageWell6 ], ebx );
	mov( roomEBX.impedeWest, ebx );
	mov( eax, impedimentEBX.key );
	

	// put bottle in grassroof hut r#18:

	createPossession
	(
