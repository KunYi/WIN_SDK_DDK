	mov( &verb_use, possessionEAX.actions[ uns32( v_use )*4 ] );
	mov( &verb_light, possessionEAX.actions[ uns32( v_light )*4 ] );
	mov( &verb_extinguish, possessionEAX.actions[ uns32( v_extinguish )*4 ] );
	
	// Getting the lantern (the first time, anyway) completes the lantern quest.
	
	mov( &quest_lantern, possessionEAX.actions[ uns32( v_get )*4 ] );

	// put rope in storage room r#9:

	createPossession
	(
